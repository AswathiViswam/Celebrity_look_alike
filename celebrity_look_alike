        "import gradio as gr\n",
        "from PIL import Image\n",
        "from deepface import DeepFace\n",
        "\n",
        "def find_and_display(img):\n",
        "    img.save(\"uploaded.jpg\")  # Save uploaded image\n",
        "    result = DeepFace.find(img_path=\"uploaded.jpg\", db_path=\"deepface/celebrities\", enforce_detection=False)\n",
        "\n",
        "    if result[0].empty:\n",
        "        return [None, None]\n",
        "\n",
        "    top = result[0].head(2)[\"identity\"].values\n",
        "    return Image.open(top[0]), Image.open(top[1])\n",
        "\n",
        "gr.Interface(\n",
        "    fn=find_and_display,\n",
        "    inputs=gr.Image(type=\"pil\"),\n",
        "    outputs=[gr.Image(), gr.Image()],\n",
        "    title=\"Celebrity Look-alike Finder\",\n",
        "    description=\"Upload a face image to find the top 2 similar celebrity matches.\"\n",
        ").launch()\n"
