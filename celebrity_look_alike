      "cell_type": "code",
      "source": [
        "#without Ui(gradio)\n",
        "from google.colab import files\n",
        "from PIL import Image\n",
        "import matplotlib.pyplot as plt\n",
        "from deepface import DeepFace\n",
        "\n",
        "# 1. Upload image\n",
        "uploaded = files.upload()\n",
        "\n",
        "# 2. Get the uploaded file name\n",
        "img_path = list(uploaded.keys())[0]\n",
        "\n",
        "# 3. Run DeepFace to find top 2 celebrity matches\n",
        "result = DeepFace.find(img_path=img_path, db_path=\"deepface/celebrities\", enforce_detection=False)\n",
        "\n",
        "# 4. Display input + top 2 matches\n",
        "def show_celebrity_matches(img_path, result_df):\n",
        "    if len(result_df) == 0 or result_df.empty:\n",
        "        print(\"No matches found.\")\n",
        "        return\n",
        "\n",
        "    matches = result_df.head(2)[\"identity\"].values\n",
        "    uploaded_img = Image.open(img_path)\n",
        "\n",
        "    plt.figure(figsize=(10,3))\n",
        "    plt.subplot(1, 3, 1)\n",
        "    plt.imshow(uploaded_img)\n",
        "    plt.title(\"Input\")\n",
        "    plt.axis(\"off\")\n",
        "    for i, path in enumerate(matches):\n",
        "        plt.subplot(1, 3, i+2)\n",
        "        plt.imshow(Image.open(path))\n",
        "        plt.title(f\"Match {i+1}\")\n",
        "        plt.axis(\"off\")\n",
        "    plt.tight_layout()\n",
        "    plt.show()\n",
        "\n",
        "show_celebrity_matches(img_path, result[0])\n"
      ],
      "metadata": {
        "id": "HfoPJhlwNHhC",
        "outputId": "f6c7d68b-ce29-45da-da87-f27925ef1c5b",
        "colab": {
          "base_uri": "https://localhost:8080/",
